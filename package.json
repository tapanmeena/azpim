{
  "name": "azpim",
  "version": "1.6.3",
  "description": "Terminal app to manage Azure PIM role activations",
  "main": "dist/index.js",
  "bin": {
    "azpim": "./dist/index.js"
  },
  "scripts": {
    "build": "tsc",
    "start": "node dist/index.js",
    "dev": "ts-node -r tsconfig-paths/register src/index.ts",
    "lint": "tsc -p tsconfig.json --noEmit",
    "release": "standard-version",
    "release:dry": "standard-version --dry-run"
  },
  "keywords": [
    "azure",
    "pim",
    "privileged-identity-management",
    "cli",
    "terminal"
  ],
  "author": {
    "name": "Tapan Meena",
    "email": "tapanmeena1998@gmail.com"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/tapanmeena/azpim.git"
  },
  "homepage": "https://github.com/tapanmeena/azpim#readme",
  "license": "ISC",
  "packageManager": "pnpm@10.29.3",
  "dependencies": {
    "@azure/arm-authorization": "^9.0.0",
    "@azure/arm-resources-subscriptions": "^2.1.0",
    "@azure/identity": "^4.13.0",
    "@inquirer/search": "^4.1.0",
    "chalk": "^5.6.2",
    "commander": "^14.0.2",
    "figures": "^6.1.0",
    "gradient-string": "^3.0.0",
    "inquirer": "^13.2.1",
    "ora": "^9.1.0"
  },
  "devDependencies": {
    "@types/node": "^25.0.10",
    "standard-version": "^9.5.0",
    "ts-node": "^10.9.2",
    "tsconfig-paths": "^4.2.0",
    "typescript": "^5.9.3"
  }
}
